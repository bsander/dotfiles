{
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "false"
  },
  "permissions": {
    "defaultMode": "plan"
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": false,
    "superpowers-chrome@superpowers-marketplace": false,
    "elements-of-style@superpowers-marketplace": false,
    "episodic-memory@superpowers-marketplace": false
  },
  "alwaysThinkingEnabled": true,
  "statusLine": {
    "type": "command",
    "command": "$HOME/.claude/statusline-command.sh"
  }
}