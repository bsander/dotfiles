{
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.run": "onType",
  "eslint.autoFixOnSave": true,
  "prettier.formatterPriority": -1,
  "prettier.eslintIntegration": false,
  "prettier.requireConfig": true,
  "suggest.triggerAfterInsertEnter": true,
  "coc.source.around.enable": false,
  "coc.source.around.priority": 0,
  "coc.source.buffer.enable": false,
  "coc.source.emoji.filetypes": null,
  "yank.enableCompletion": false,
  "yank.priority": 0,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "json"
  ],
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck",
    "bash": "shellcheck",
    "zsh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt",
    "bash": "shfmt",
    "zsh": "shfmt"
  }
}
